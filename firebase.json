{
    "hosting": [
        {
            "target":"v2",
            "site": "growthfile-webapp",
            "public": "website",
            "cleanUrls": true,
            "trailingSlash": false,
            "ignore": [
                "firebase.json",
                "explorer firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "/api",
                    "function": "api"
                },
                {
                    "source": "/**",
                    "function": "webapp"
                }
            ]
        }
    ],
    "functions": {
        "source": "functions"
    }
}
