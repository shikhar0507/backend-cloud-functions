<!DOCTYPE html>
<html lang="en">

<head>
    \{{> headPartial this}}
    <link rel="stylesheet" href="css/download.css">
</head>

<body>
    \{{> persistentBarPartial this}}
    \{{> headerPartial this}}
    <div class="container">
        \{{> asidePartial}}
        <h1>Download Growthfile App</h1>
        <section id="ios-link">
            <a href="https://play.google.com/store/apps/details?id=com.growthfile.growthfileNew">
                <img src="img/play-store-icon.jpg" alt="google play store icon">
            </a>
        </section>
        <section id="android-link">
            <a href="https://itunes.apple.com/in/app/growthfile/id1441388774?mt=8">
                <img src="img/app-store-icon.jpg" alt="apple app store icon">
            </a>
        </section>
    </div>
    \{{> scriptsPartial}}
    <script defer src="js/download.js"></script>
</body>

</html>
